<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="STypeMapper">
	
	<!--表名 -->
	<sql id="tableName">
		TB_STYPE
	</sql>
	
	<!-- 字段 -->
	<sql id="Field">
		COMPANY_ID,	
		TYPE_NAME,	
		REMARK,	
		YL1,	
		YL2,	
		YL3,	
		YL4,
		YL5,
		YL6,
		YL7,
		YL8,
		YL9,
		YL10,
		YL11,	
		YL12,	
		YL13,	
		YL14,
		YL15,
		YL16,
		YL17,
		YL18,
		YL19,
		YL20,
		STYPE_ID
	</sql>
	
	<!-- 字段值 -->
	<sql id="FieldValue">
		#{COMPANY_ID},	
		#{TYPE_NAME},	
		#{REMARK},	
		#{YL1},	
		#{YL2},	
		#{YL3},	
		#{YL4},
		#{YL5},
		#{YL6},	
		#{YL7},	
		#{YL8},	
		#{YL9},
		#{YL10},
		#{YL11},	
		#{YL12},	
		#{YL13},	
		#{YL14},
		#{YL15},
		#{YL16},	
		#{YL17},	
		#{YL18},	
		#{YL19},
		#{YL20},	
		#{STYPE_ID}
	</sql>
	
	<!-- 新增-->
	<insert id="save" parameterType="pd">
		insert into 
	<include refid="tableName"></include>
		(
	<include refid="Field"></include>
		) values (
	<include refid="FieldValue"></include>
		)
	</insert>
	
	<!-- 删除-->
	<delete id="delete" parameterType="pd">
		delete from
		<include refid="tableName"></include>
		where 
			STYPE_ID = #{STYPE_ID}
	</delete>
	
	<!-- 修改 -->
	<update id="edit" parameterType="pd">
		update
		<include refid="tableName"></include>
		set 
			COMPANY_ID = #{COMPANY_ID},
			TYPE_NAME = #{TYPE_NAME},
			REMARK = #{REMARK},
			YL1 = #{YL1},
			YL2 = #{YL2},
			YL3 = #{YL3},
			YL4 = #{YL4},
			YL5 = #{YL5},
			YL6 = #{YL6},
			YL7 = #{YL7},
			YL8 = #{YL8},
			YL9 = #{YL9},
			YL10 = #{YL10},
			YL11 = #{YL11},
			YL12 = #{YL12},
			YL13 = #{YL13},
			YL14 = #{YL14},
			YL15 = #{YL15},
			YL16 = #{YL16},
			YL17 = #{YL17},
			YL18 = #{YL18},
			YL19 = #{YL19},
			YL20 = #{YL20},
		STYPE_ID = STYPE_ID
		where 
		STYPE_ID = #{STYPE_ID}
	</update>
	
	<!-- 通过ID获取数据 -->
	<select id="findById" parameterType="pd" resultType="pd">
		select 
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 
			STYPE_ID = #{STYPE_ID}
	</select>
	
	<!-- 列表 -->
	<select id="datalistPage" parameterType="page" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 1=1
		<if test="pd.keywords!= null and pd.keywords != ''"><!-- 关键词检索 -->
			and
				(
					TYPE_NAME LIKE CONCAT(CONCAT('%', #{pd.keywords}),'%')
				)
		</if>
		<if test="pd.name != null and pd.name != ''">
			and
				(
					TYPE_NAME LIKE CONCAT(CONCAT('%', #{pd.name}),'%')
				)
		</if>
		<if test="pd.COMPANY_ID != null and pd.COMPANY_ID != ''">
		             
			and
				(
					COMPANY_ID = #{pd.COMPANY_ID}
				)
		</if>
	</select>
	
	<!-- 列表(全部) -->
	<select id="listAll" parameterType="pd" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 1=1
		<if test="COMPANY_ID != null and COMPANY_ID != ''">
		             
			and
				(
					COMPANY_ID = #{COMPANY_ID}
				)
		</if>
	</select>
	
	<!-- 批量删除 -->
	<delete id="deleteAll" parameterType="String">
		delete from
		<include refid="tableName"></include>
		where 
			STYPE_ID in
		<foreach item="item" index="index" collection="array" open="(" separator="," close=")">
                 #{item}
		</foreach>
	</delete>
	
	<select id="findByCondition" parameterType="page" resultType="pd">
		SELECT
		*
		FROM 
		<include refid="tableName"></include>
		where
		1=1 
		<if test="pd.COMPANY_ID != null and pd.COMPANY_ID != ''">
			and COMPANY_ID = #{pd.COMPANY_ID}
		</if>
	</select>
	
	
	<!-- fh313596790qq(青苔) -->
</mapper>