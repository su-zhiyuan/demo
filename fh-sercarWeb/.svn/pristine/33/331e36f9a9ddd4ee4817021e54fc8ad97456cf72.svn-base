<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="GoodsMapper">
	
	<!--表名 -->
	<sql id="tableName">
		TB_GOODS
	</sql>
	
	<!-- 字段 -->
	<sql id="Field">
		CAIGOURC_ID,	
		NAME,	
		NUMBER,	
		PRICE,	
		CREACTE_TIME,	
		REASON,	
		YL1,	
		YL2,	
		YL3,	
		YL4,	
		YL5,	
		YL6,	
		YL7,	
		YL8,	
		YL9,	
		YL10,	
		YL11,	
		YL12,	
		YL13,	
		YL14,	
		YL15,	
		YL16,	
		YL17,	
		YL18,	
		YL19,	
		YL20,	
		GOODSRC_ID
	</sql>
	
	<!-- 字段值 -->
	<sql id="FieldValue">
		#{CAIGOURC_ID},	
		#{NAME},	
		#{NUMBER},	
		#{PRICE},	
		#{CREACTE_TIME},	
		#{REASON},	
		#{YL1},	
		#{YL2},	
		#{YL3},	
		#{YL4},	
		#{YL5},	
		#{YL6},	
		#{YL7},	
		#{YL8},	
		#{YL9},	
		#{YL10},	
		#{YL11},	
		#{YL12},	
		#{YL13},	
		#{YL14},	
		#{YL15},	
		#{YL16},	
		#{YL17},	
		#{YL18},	
		#{YL19},	
		#{YL20},	
		#{GOODSRC_ID}
	</sql>
	
	<!-- 新增-->
	<insert id="save" parameterType="pd">
		insert into 
	<include refid="tableName"></include>
		(
	<include refid="Field"></include>
		) values (
	<include refid="FieldValue"></include>
		)
	</insert>
	
	<!-- 删除-->
	<delete id="delete" parameterType="pd">
		delete from
		<include refid="tableName"></include>
		where 
			GOODSRC_ID = #{GOODS_ID}
	</delete>
	
	<!-- 修改 -->
	<update id="edit" parameterType="pd">
		update
		<include refid="tableName"></include>
		set 
			CAIGOURC_ID = #{CAIGOURC_ID},
			NAME = #{NAME},
			NUMBER = #{NUMBER},
			PRICE = #{PRICE},
			CREACTE_TIME = #{CREACTE_TIME},
			REASON = #{REASON},
			YL1 = #{YL1},
			YL2 = #{YL2},
			YL3 = #{YL3},
			YL4 = #{YL4},
			YL5 = #{YL5},
			YL6 = #{YL6},
			YL7 = #{YL7},
			YL8 = #{YL8},
			YL9 = #{YL9},
			YL10 = #{YL10},
			YL11 = #{YL11},
			YL12 = #{YL12},
			YL13 = #{YL13},
			YL14 = #{YL14},
			YL15 = #{YL15},
			YL16 = #{YL16},
			YL17 = #{YL17},
			YL18 = #{YL18},
			YL19 = #{YL19},
			YL20 = #{YL20},
		GOODSRC_ID = GOODSRC_ID
		where 
		GOODSRC_ID = #{GOODSRC_ID}
	</update>
	
	<!-- 通过ID获取数据 -->
	<select id="findById" parameterType="pd" resultType="pd">
		select 
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 
			GOODSRC_ID = #{GOODSRC_ID}
	</select>
	
	<!-- 列表 -->
	<select id="datalistPage" parameterType="page" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 1=1
		<if test="pd.keywords!= null and pd.keywords != ''"><!-- 关键词检索 -->
			and
				(
				
					NAME LIKE CONCAT(CONCAT('%', #{pd.keywords}),'%')
					 or 
					CAIGOURC_ID LIKE CONCAT(CONCAT('%', #{pd.keywords}),'%') 
				
				)
		</if>
		<if test="pd.lastStart != null and pd.lastStart != '' and pd.lastEnd != null and pd.lastEnd != ''">
		and
			(
					CREACTE_TIME &gt;= #{pd.lastStart} and
					CREACTE_TIME &lt;= #{pd.lastEnd}
			)
		</if>
		<if test="pd.YL10 != null and pd.YL10 != ''">
			and
				(
					YL10 =#{pd.YL10}
				)
		</if>
		order by CREACTE_TIME desc
	</select>
	
	<!-- 列表(全部) -->
	<select id="listAll" parameterType="pd" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
	</select>
	
	<!-- 批量删除 -->
	<delete id="deleteAll" parameterType="String">
		delete from
		<include refid="tableName"></include>
		where 
			GOODSRC_ID in
		<foreach item="item" index="index" collection="array" open="(" separator="," close=")">
                 #{item}
		</foreach>
	</delete>
	
	<!-- fh313596790qq(青苔) -->
</mapper>